    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.0",
        "firebase/php-jwt": "^6.3",
        "phpseclib/phpseclib": "^3.0",
        "pragmarx/google2fa": "^8.0",
        "abraham/twitteroauth": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "MoneyPaws\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "serve": "php -S localhost:8000 -t ."
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
